{
  "builds": [
    {
      "src": "**/*.dart",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "flutter build web --dart-define=API_BASE_URL=$BACKEND_URL",
        "distDir": "build/web"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}